<h1>Episode <%= episode %> , Season <%= season %> of <%= series %></h2>
<% filtered.forEach((episode) => { %>
    <h2>Title: <%= episode.title %></h2>
<% }) %>

<table>
    <tr>
        <td>
            <a>#</a>
        </td>
        <td>
            <a>Title</a>
        </td>
        <td>
            <a>Air date</a>
        </td>
        <td>
            <a>Characters</a>
        </td>
    </tr>
    <% filtered.forEach((episode) => { %>
      <tr>
        <td>
            <a><%= episode.episode %></a>
        </td>
        <td>
            <a><%= episode.title %></a>
        </td>
        <td>
            <a><%= episode.air_date %></a>
        </td>
        <td>
            <% episode.characters.forEach((character) => { %>
                <a href="<%= showCharacterPath(character) %>"><%= character %> , </a>
            <% }) %>
        </td>
      </tr>
    <% }) %>
  </table>